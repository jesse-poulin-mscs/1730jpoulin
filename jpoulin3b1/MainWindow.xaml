            <Label Content="7) TotalHours(string)" HorizontalAlignment="Left" VerticalAlignment="Center" Height="28" Grid.Row="7"/>
            <TextBox x:Name="inputTextBox7a" Grid.Column="1" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="161" Grid.Row="7" Text="8 10 11 11 10 " Grid.ColumnSpan="2" Margin="0.222,4.556,0,3.889"/>
            <TextBox x:Name="resultTextBox7" Grid.Column="3" HorizontalAlignment="Left" Height="24" TextWrapping="Wrap" VerticalAlignment="Center" Width="105" Grid.ColumnSpan="2" Grid.Row="7" Margin="114.778,5.444,0,5.667"/>
            <Label Content="8) GrossPay(string, dec)" HorizontalAlignment="Left" VerticalAlignment="Center" Height="28" Grid.Row="8"/>
